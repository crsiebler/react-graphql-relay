{
  "name": "rgrjs",
  "version": "1.0.0",
  "description": "A collection of educational resources about React, GraphQL, and Relay",
  "scripts": {
    "build": "babel src/server -d dist",
    "relay": "relay-compiler --src ./src --schema ./data/schema.json",
    "start": "run-p start:dev start:api",
    "start:dev": "webpack-dev-server --config webpack.config.dev.js --port 3000",
    "prestart:dev": "npm run relay",
    "start:api": "nodemon --ext js --exec babel-node server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crsiebler/rgrjs.git"
  },
  "keywords": [
    "React",
    "GraphQL",
    "Relay",
    "Flux",
    "Node",
    "Express"
  ],
  "author": "Cory Siebler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/crsiebler/rgrjs/issues"
  },
  "homepage": "https://github.com/crsiebler/rgrjs#readme",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "axios": "^0.19.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-relay": "^8.0.0",
    "cors": "^2.8.5",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.18.0",
    "events": "^3.1.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.6.0",
    "graphql-relay": "^0.6.0",
    "html-webpack-plugin": "^3.2.0",
    "kerberos": "^1.1.3",
    "mongodb": "^3.5.2",
    "npm-run-all": "^4.1.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-relay": "^8.0.0",
    "relay-compiler": "^8.0.0",
    "relay-config": "^8.0.0",
    "relay-runtime": "^8.0.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
